{
  "name": "MultiApp-BaseProject",
  "version": "0.2.42",
  "description": "This is a little repository that bring you a development environment to create a project with Angular, SASS (optional) (etc...). You can generate multiple app thanks to Cordova or Electron.",
  "scripts": {
    "app:clean-web": "gulp clean-web",
    "app:clean-electron": "gulp clean-electron",
    "app:clean-cordova": "gulp clean-cordova",
    "app:build-web": "gulp build-web",
    "app:build-electron": "gulp build-electron",
    "app:build-cordova": "gulp build-cordova",
    "app:watch-web": "gulp watch-web",
    "electron:start": "electron dist/electron/www",
    "cordova:android-start": "cordova run android",
    "ltserver:start": "lite-server --baseDir ./dist/www --port 3333",
    "electron:pack-win64": "electron-packager ./dist/electron/www BaseProject --platform=win32 --arch=x64 --version=0.33.3"
  },
